{
    "name": "aldesrahim/tabbed-panel",
    "description": "This is my package tabbed-panel",
    "keywords": [
        "aldesrahim",
        "laravel",
        "filamentphp",
        "tabbed-panel"
    ],
    "homepage": "https://github.com/aldesrahim/tabbed-panel",
    "support": {
        "issues": "https://github.com/aldesrahim/tabbed-panel/issues",
        "source": "https://github.com/aldesrahim/tabbed-panel"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Al Desrahim",
            "email": "aldesrahim@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.6",
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^10.6",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "rector/rector": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Aldesrahim\\TabbedPanel\\": "src/",
            "Aldesrahim\\TabbedPanel\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aldesrahim\\TabbedPanel\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare",
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "analyse": "phpstan analyse",
        "lint": "pint",
        "refactor": "rector",
        "test": "pest",
        "test:lint": "pint --test",
        "test:refactor": "rector --dry-run",
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "ide-helper:generate": "@php ./vendor/bin/testbench ide-helper:generate",
        "ide-helper:models": "@php ./vendor/bin/testbench ide-helper:models --dir=\"src/Models\" -RW"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aldesrahim\\TabbedPanel\\TabbedPanelServiceProvider"
            ],
            "aliases": {
                "TabbedPanel": "Aldesrahim\\TabbedPanel\\Facades\\TabbedPanel"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
